{
  "manifest_version": 3,
  "name": "Tab Switcher 0.1ms",
  "version": "1.0",
  "permissions": ["tabs", "windows", "storage"],
  "action": {
    "default_icon": {
      "16": "images/icon.png",
      "48": "images/icon.png",
      "128": "images/icon.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "options.html"
}
